Exercicio 1:

void main() {

   for(int i = 0; i<=100; i++)
   {
    print(i);
  }

}


Exercicio 2:

void main() {
  
   int maior= 0;

   for(int i = 0; i<=10; i++)
   {
    print(i);
     
     if (maior<i) {
       maior=i;
       print(maior);
           
     }
  }

}


Exercicio 3:

void main() {
  int n1= 2;
  int n2= 10;
  int soma= n1*n2;
 

   for(int i =0; i<=1200; i+=soma)
   {
    print(i);
  }
  
}



Exercicio 4:


void main() {
  double media_turma= 0;
  int total= 0;
 
  const List media_alunos = [9, 7, 6, 7, 8, 10, 7, 8, 10, 9];

  for(int num in media_alunos ){
    print(num);
    total+=num;
    media_turma=total/media_alunos.length;
  }
      print(media_turma);
}




